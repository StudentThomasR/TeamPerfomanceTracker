@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Actemium tpt |@ViewBag.Title</title>
    <link rel="stylesheet" type="text/css" href="~/Content/mobile.css" />
    <link rel="stylesheet"
          media="only screen and (min-width:1000px)"
          href="~/Content/desktop.css" />



</head>
<body>
    <div class="container">
        <header>
            <nav>
                <ul>
                    <li><a href="#"><img class="logo" src="~/Content/Images/logo.jpeg" /></a></li>
                    <li><h2 class="name">Automation Team Performance Tracker</h2></li>
                    <li><p class="page-name">@ViewBag.Title</p></li>
                </ul>
            </nav>
        </header>
    </div>
    <div class="container">
        @RenderBody()
    </div>

    <script src="https://cdn.jsdelivr.net/npm/chart.js@2.9.4/dist/Chart.min.js"></script><!--ChartJS CDN-->
    <script src="https://cdn.jsdelivr.net/npm/chartjs-plugin-datalabels@0.7.0"></script><!--Data labels plugin-->
    <script src="~/Content/js/barchart.js"></script>
    <script src="~/Content/js/linechartTeam.js"></script>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>